<h1>My lists</h1>
<% @lists.each do |list| %>
  <div class="card-product">
  <%if list.photo.attached? %>
    <div class ="list-image"><%= cl_image_tag list.photo.key, class: "list-image" %></div>
   <% end %>
    <div class="card-product-infos">
      <h2><%= link_to "#{list.name}", list_path(list.id) %></h2>
    </div>
  </div>
<% end %>

<p>

    <p></p>

</p>
<%= link_to "Add a new list", new_list_path, class: "btn btn-secondary" %>
