<nav class="navbar">
    <div class="navbar-button"><%= link_to "ADD A NEW LIST", new_list_path %></div>
</nav>
<h1>My lists</h1>
<% @lists.each do |list| %>
  <%= link_to list_path(list.id) do %>
    <div class="card-product">
      <%if list.photo.attached? %>
        <div class ="list-image"><%= cl_image_tag list.photo.key, class: "list-image" %></div>
      <% end %>
      <div class="card-product-infos">
        <h2><%= "#{list.name}"%></h2>
      </div>
    </div>
  <% end %>
<% end %>

<p>

    <p></p>

</p>
